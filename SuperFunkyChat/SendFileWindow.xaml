<Window x:Class="SuperFunkyChat.SendFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Send File Window" Height="155" Width="496" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="77,12,0,0" Name="textBoxUserName" VerticalAlignment="Top" Width="298" />
        <Label Content="UserName:" Height="28" HorizontalAlignment="Left" Margin="12,10,0,0" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="77,44,0,0" Name="textBoxFileName" VerticalAlignment="Top" Width="298" />
        <Button Content="Browse" Height="23" HorizontalAlignment="Left" Margin="387,43,0,0" Name="btnBrowse" VerticalAlignment="Top" Width="75" Click="btnBrowse_Click" />
        <Label Content="FileName:" Height="28" HorizontalAlignment="Left" Margin="18,42,0,0" VerticalAlignment="Top" />
        <Button Content="Send" Height="23" HorizontalAlignment="Left" Margin="138,81,0,0" Name="btnSend" VerticalAlignment="Top" Width="75" IsDefault="True" Click="btnSend_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="259,81,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" IsCancel="True" Click="btnCancel_Click" />
    </Grid>
</Window>
